<style>
#dialogoverlay{
  display: none;
  opacity: .1;
  position: fixed;
  top: 1px;
  left: 1px;
  background-repeat:  #333;
  text-align: center;
  width: 30%;
  z-index: 10;
}
#dialogbox{
  text-align: center;
  display: none;
  position: fixed;
  background: #000;
  border-radius:2px; 
  width:380px;
  z-index: 10;
  width: 30%;
  
}
#dialogbox > div{ background:#FFF; margin:8px; }
#dialogbox > div > #dialogboxhead{ background: #666; font-size:8px; padding:10px; color:#CCC; }
#dialogbox > div > #dialogboxbody{ background:#333; padding:5px; color:#FFF; }
#dialogbox > div > #dialogboxfoot{ background: #666; padding:5px; text-align:center; }
</style>
<script>
function CustomAlert(){
    this.render = function(dialog){
        var winW = window.innerWidth;
        var winH = window.innerHeight;
        var dialogoverlay = document.getElementById('dialogoverlay');
        var dialogbox = document.getElementById('dialogbox');
        dialogoverlay.style.display = "block";
        dialogoverlay.style.height = winH+"px";
        dialogbox.style.left = (winW/2) - (550 * .5)+"px";
        dialogbox.style.top = "100px";
        dialogbox.style.display = "block";
        document.getElementById('dialogboxhead').innerHTML = "Welcome to ∞∞Gads∞∞ toolbox! here you can get ∞Gads∞ Gadgetry. This is the browsers toolbox, itll transmorph as new gadgets cross over to new platforms, and etc.∞Gads∞ superpower is that he is his own infinite sized toolbox by utilizing compression algorithms! leave a tool suggestion in his Gmail box.Beta: Submit ∞Gads∞ fanart, he wants us to depict ∞Gads∞ because he is virtual and has no arms!.";
        document.getElementById('dialogboxbody').innerHTML = "Friends";
        document.getElementById('dialogboxfoot').innerHTML = '<button onclick="Alert.ok()">ARM</button>';
    }
  this.ok = function(){
    document.getElementById('dialogbox').style.display = "none";
    document.getElementById('dialogoverlay').style.display = "none";
  }
}
var Alert = new CustomAlert();
</script>




